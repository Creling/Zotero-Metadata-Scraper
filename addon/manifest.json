{
  "manifest_version": 2,
  "name": "Zotero Metadata Scraper",
  "version": "1.0.0",
  "description": "A Zotero plugin to fetch metadata from multiple sources",
  "homepage_url": "https://github.com/creling/zotero-metadata-scraper",
  "author": "creling",
  "icons": {
    "48": "content/icons/favicon@0.5x.png",
    "96": "content/icons/favicon.png"
  },
  "applications": {
    "zotero": {
      "id": "__addonID__",
      "update_url": "__updateURL__",
      "strict_min_version": "7.0",
      "strict_max_version": "7.*"
    }
  }
}
